{
  "name": "course-api",
  "version": "1.0.0",
  "description": "application for course online",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.js --exec babel-node src/app.js ",
    "build": "rimraf build && babel src -d build",
    "serve": "node src/app.js",
    "pm2": "pm2 start build/app.js --name api_hris",
    "build:server": "npm run build && npm run pm2",
    "build:restart": "npm run build && npm run pm2:restart",
    "pm2:start": "pm2 start api_hris",
    "pm2:delete": "pm2 delete api_hris",
    "pm2:stop": "pm2 stop api_hris",
    "pm2:restart": "pm2 restart api_hris"
  },
  "keywords": [
    "course"
  ],
  "author": "arif permadi",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "babel-plugin-dotenv-import": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "express-group-router": "^2.1.0",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.1.6",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1"
  }
}
